  <%= simple_form_for(@planet) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price_per_night %>
    <%= f.input :population %>
    <%= f.input :planet_type %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, class: 'btn btn-primary mb-3' %>
  <% end %>
